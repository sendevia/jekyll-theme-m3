source 'https://rubygems.org'

gem 'jekyll'
gem "jekyll-sass-converter", "~> 3.0"
group :jekyll_plugins do
  gem 'jekyll-spaceship'
  gem 'jekyll-seo-tag'
  gem 'jekyll-sitemap'
end

platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem 'tzinfo'
  gem 'tzinfo-data'
end

gem 'wdm', :install_if => Gem.win_platform?

gem 'webrick'
